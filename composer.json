{
    "name": "lucasmarin/theme-installer",
    "description": "A composer-installer for Roundcube themes.",
    "type": "composer-installer",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Thomas Bruederli",
            "email": "thomas@roundcube.net"
        },
        {
            "name": "Till Klampaeckel",
            "email": "till@php.net"
        },
        {
            "name": "Lucas Stevanelli Marin",
            "email": "lucas.marin@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Roundcube\\Composer": "src/"
        }
    },
    "extra": {
        "class": "Roundcube\\Composer\\ThemeInstaller"
    },
    "bin": [
        "src/bin/rcubeinitdb.sh"
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "composer/composer": "*"
    }
}
